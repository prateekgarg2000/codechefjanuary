//https://www.codechef.com/JAN20B/problems/DYNAMO
#include <iostream>
using namespace std;
long long power(int n)
{
    if(n==0)
    return 1;
    return power(n-1)*10;
}
int main() {
	// your code goes here
	int t,n,r;
	long long a,b,c,e,s,d,p;
	cin>>t;
	while(t>0)
	{
	    cin>>n;
	    p=power(n);
	    cin>>a;
	    s=2*p+a;
	    cout<<s<<endl;
	    cin>>b;
	    c=p-b;
	    cout<<c<<endl;
	    cin>>d;
	    e=p-d;
	    cout<<e<<endl;
	    
	    cin>>r;
	    t--;
	}
	return 0;
}
